{
  "name": "downloads.gravitee.io",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Julien Giovaresco <dev@giovaresco.fr>",
  "main": "index.js",
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "lint:prettier": "prettier --check '**/*.{js,ts,json}'",
    "lint:prettier:fix": "prettier --write \"**/*.{js,ts,json}\"",
    "start": "serve dist",
    "start:dev": "npm-run-all --parallel start build:watch"
  },
  "lint-staged": {
    "*.{js,html,css,json}": "prettier --write"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "prettier-plugin-packagejson": "^2.2.15",
    "rollup": "^2.36.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-html-bundle": "^0.0.3",
    "rollup-plugin-terser": "^7.0.2",
    "serve": "^13.0.2"
  },
  "packageManager": "yarn@3.1.0",
  "dependencies": {
    "date-fns": "^2.26.0"
  }
}
